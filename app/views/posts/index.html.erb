
<h4>タグリスト</h4>
<% @tag_list.each do |list| %>
<span>
  <%= link_to list.tag_name, tag_posts_path(tag_id: list.id) %>
  <%= "(#{list.posts.count})" %>
</span>
<% end %>

<div class="title">
  <div class="container">
    <h4>投稿一覧</h4>
  </div>
</div>

<div class="container">
<% @posts.each do |post| %>

 <%= render 'post', post: post %>

<% end %>
</div>